{
    "Comment": "State machine to run a single ETL run group",
    "StartAt": "Init",
    "States": {
      "Init": {
        "Comment": "Create the run map to initialize the run.",
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-1:382274149743:function:create_run_map",
        "Parameters": {
          "s3bucket": "managed-data-assets-dev",
          "rungroup": "daily"
        },
        "ResultPath": "$",
        "Next": "Check200"
      },
      "Check200": {
        "Comment": "Make sure there wasn't an error return",
        "Type": "Choice",
        "Choices": [
          {
            "Variable": "$.statusCode",
            "NumericEquals": 200,
            "Next": "IsGo"
          }
        ],
        "Default": "Failed",
        "OutputPath": "$.body"
      },
      "IsGo": {
        "Comment": "Make sure there wasn't an error return",
        "Type": "Choice",
        "Choices": [
          {
            "Variable": "$.go",
            "BooleanEquals": true,
            "Next": "ProcessSet"
          },
          {
            "Not": {
              "Variable": "$.go",
              "BooleanEquals": true
            },
            "Next": "Done"
          }
        ],
        "Default": "Done"
      },
      "ProcessSet": {
        "Type": "Map",
        "MaxConcurrency": 10,
        "InputPath": "$",
        "ItemsPath": "$.next",
        "Parameters": {
          "ETLJobIndex.$": "$$.Map.Item.Index",
          "ETLJob.$": "$$.Map.Item.Value"
        },
        "Iterator": {
          "StartAt": "ProcessOneJob",
          "States": {
            "ProcessOneJob": {
              "Type": "Map",
              "MaxConcurrency": 1,
              "InputPath": "$",
              "ItemsPath": "$.ETLJob.etl_tasks",  
              "Parameters": {
                "ETLTaskIndex.$": "$$.Map.Item.Index",
                "ETLTask.$": "$$.Map.Item.Value"
              },
              "OutputPath": "$",
              "Iterator": {
                "StartAt": "DoOneTask",
                "States": {
                  "DoOneTask": {
                    "Type": "Pass",
                    "ResultPath": "$.out",
                    "End": true
                  }
                }
              },
              "ResultPath": "$.jobresults",
              "End": true
            }
          }
        },
        "Next": "Done",
        "ResultPath": "$.results"
      },
      "Failed": {
        "Type": "Fail",
        "Cause": "Not Hello World"
      },
      "Done": {
        "Type": "Succeed"
      }
    }
  }
  